<script type="text/javascript"> 
$(function(){
  $(".content_left .menuli .a1").bind("click",function(){
    if($(this).parent().find(".twoul").length>0){  
      var flagli = $(this).parent(".open").length;
      if(flagli>0){
        $(this).parent().removeClass("open"); 
      }else{        
        $(this).parent().parent().find(".open").removeClass("open"); 
        $(this).parent().addClass("open"); 
      }      
    }
  });
  $(".content_left .menuli .twoul .twoli").bind("click",function(){
    $(this).parents(".content_left").find(".cur").removeClass("cur"); 
    $(this).addClass("cur"); 
  });

});
</script>
